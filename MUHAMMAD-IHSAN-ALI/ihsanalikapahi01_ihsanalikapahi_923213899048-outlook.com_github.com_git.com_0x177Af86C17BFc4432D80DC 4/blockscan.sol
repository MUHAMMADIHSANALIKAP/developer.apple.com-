<script type="text/javascript">
    document.addEventListener('DOMContentLoaded', function () {
      let currentWordIndex = 0;
      const words = document.querySelectorAll('.js-animated-word');
      // Initially show the first word
      words.forEach((word, index) => {
        word.style.display = index === 0 ? 'inline-block' : 'none';
      });
      updateBackgroundClass(currentWordIndex); // Set initial background class

      function cycleWords() {
        // Hide the current word
        words[currentWordIndex].style.display = 'none';
        // Move to the next word
        currentWordIndex = (currentWordIndex + 1) % words.length;
        // Show the next word
        words[currentWordIndex].style.display = 'inline-block';
        // Update the background class
        updateBackgroundClass(currentWordIndex);
      }

      function updateBackgroundClass(index) {
        // Remove only bg-word-X classes
        for (let i = 0; i < words.length; i++) {
          document.body.classList.remove(`bg-word-${i}`);
        }
        // Add new background class for the current word
        document.body.classList.add(`bg-word-${index}`);
      }

      // Automatically cycle through words every 3 seconds
      setInterval(cycleWords, 3000);
    });
  </script>
<script type="text/javascript">
    document.addEventListener("DOMContentLoaded", function (event) {
      initHeader();
      (function () {
        // POPOVER
        // =======================================================
        const popoverTriggerList = [].slice.call(document.querySelectorAll('[data-bs-toggle="popover"]'))
        const myDefaultAllowList = bootstrap.Tooltip.Default.allowList;
        myDefaultAllowList.img = ['src', 'srcset', 'alt', 'title', 'width', 'height', 'data-tokenalt'];
        popoverTriggerList.map((el) => {
          let opts = {
            animation: true,
            allowList: myDefaultAllowList
          }
          if (el.hasAttribute('data-bs-content-id')) {
            opts.content = document.getElementById(el.getAttribute('data-bs-content-id'))
              .innerHTML
              .replace(/data-cfsrc/g, 'src')
              .replace(/style="display:none;visibility:hidden;"/g, '');
            opts.html = true;
          }
          new bootstrap.Popover(el, opts);
          el.addEventListener('shown.bs.popover', function (a) {
            genericImageErrorHandling();
          });

        })
      })();

      function getClientTimezoneOffset() {
        return -(new Date().getTimezoneOffset() / 60);
      }

      // Store timezone offset in a cookie
      document.cookie = "multiblockscan_timeoffset=" + getClientTimezoneOffset() + "; path=/";
    });

    document.addEventListener("DOMContentLoaded", function (event) {
